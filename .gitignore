# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
api/log/*
!/api/log/.keep
logs/*
!/logs/.keep
data/*
!/data/.keep
/tmp
/public
/.DS_Store
/.idea
/.vagrant
/vendor/bundle
.byebug_history
/.yardoc
/doc